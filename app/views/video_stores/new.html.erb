<form action="/video_stores" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new video store:</p>
  <p>name</p><input type='text' name='video_store[name]'/><br/>
  <p>rank</p><input type='text' name='video_store[rank]'/><br/>
  <p>flagship store?</p><input type='text' name='video_store[flagship_store]'/><br/>
  <input type='submit' value='create new video store'/>
</form>
